{
  "name": "webeyes-electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "electron ."
  },
  "author": "echekunov",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-rebuild": "^1.8.5",
    "iohook": "^0.6.0"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "6.0.0"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-73"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "description": "This is an analogue xeyes linux application based on HTML5, CSS and JS (Electron).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeforkids-ru/webeyes.git"
  },
  "keywords": [
    "electron",
    "xeyes",
    "javascript",
    "html5",
    "css"
  ],
  "bugs": {
    "url": "https://github.com/codeforkids-ru/webeyes/issues"
  },
  "homepage": "https://github.com/codeforkids-ru/webeyes#readme"
}
